{
  "name": "seedling",
  "version": "1.0.0",
  "description": "SEEDLING - Covered Buildings List Generator",
  "homepage": "https://seed-platform.org/",
  "author": {
    "name": "NREL"
  },
  "main": "main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps && node download.js",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c dev --ssl true",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "~10.0.1",
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular-eslint/builder": "~0.6.0-beta.0",
    "@angular-eslint/eslint-plugin": "~0.6.0-beta.0",
    "@angular-eslint/eslint-plugin-template": "~0.6.0-beta.0",
    "@angular-eslint/template-parser": "~0.6.0-beta.0",
    "@angular/animations": "~10.2.3",
    "@angular/cdk": "~10.2.7",
    "@angular/cli": "~10.2.0",
    "@angular/common": "~10.2.3",
    "@angular/compiler": "~10.2.3",
    "@angular/compiler-cli": "~10.2.3",
    "@angular/core": "~10.2.3",
    "@angular/flex-layout": "~10.0.0-beta.32",
    "@angular/forms": "~10.2.3",
    "@angular/language-service": "~10.2.3",
    "@angular/material": "~10.2.7",
    "@angular/platform-browser": "~10.2.3",
    "@angular/platform-browser-dynamic": "~10.2.3",
    "@angular/router": "~10.2.3",
    "@ngx-translate/core": "~13.0.0",
    "@ngx-translate/http-loader": "~6.0.0",
    "@types/jasmine": "~3.6.1",
    "@types/jasminewd2": "~2.0.8",
    "@types/mocha": "~8.0.4",
    "@types/node": "~14.14.7",
    "@types/overlayscrollbars": "~1.12.0",
    "@typescript-eslint/eslint-plugin": "~4.7.0",
    "@typescript-eslint/eslint-plugin-tslint": "~4.7.0",
    "@typescript-eslint/parser": "~4.7.0",
    "chai": "~4.2.0",
    "conventional-changelog-cli": "~2.1.1",
    "core-js": "~3.7.0",
    "cross-env": "~7.0.2",
    "electron": "~10.1.5",
    "electron-builder": "~22.9.1",
    "electron-reload": "~1.5.0",
    "eslint": "~7.13.0",
    "eslint-plugin-import": "~2.22.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~5.2.3",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-electron": "~6.3.1",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.5.4",
    "material-design-icons": "~3.0.1",
    "mocha": "~8.2.1",
    "node-stream-zip": "~1.11.7",
    "npm-run-all": "~4.1.5",
    "overlayscrollbars": "~1.13.0",
    "overlayscrollbars-ngx": "~0.2.2",
    "rxjs": "~6.6.3",
    "spectron": "~12.0.0",
    "ts-node": "~9.0.0",
    "tslib": "~2.0.3",
    "typescript": "~4.0.5",
    "wait-on": "~5.2.0",
    "webdriver-manager": "~12.1.7",
    "zone.js": "~0.11.3"
  },
  "dependencies": {
    "dotenv": "~8.2.0",
    "electron-window-state": "~5.0.3",
    "pg": "~8.5.0",
    "pg-hstore": "~2.3.3",
    "sequelize": "~6.3.5",
    "umzug": "3.0.0-beta.5"
  },
  "engines": {
    "node": ">=10.13.0"
  }
}
